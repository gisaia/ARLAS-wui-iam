    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "license-check-and-add": "^4.0.5",
    "node": "^18.19.0",
    "puppeteer": "^1.13.0",
    "typescript": "5.4.5"
  },
  "overrides": {
    "jsrsasign": "^11.0.0",
