{
  "name": "arlas-wui-iam",
  "version": "25.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href='$ARLAS_WUI_IAM_BASE_HREF/'",
    "watch": "ng build --watch --configuration development",
